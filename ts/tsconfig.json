{
    "compilerOptions": {
        "target": "ES5",
        "noEmitOnError": true,
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "module": "commonjs",
        "lib": [
            "es5",
            "dom",
            "es2015.promise"
        ]
    }
}
